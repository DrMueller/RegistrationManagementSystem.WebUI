{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "paths": {
      "@drmueller/language-extensions": [
        "dist/drmueller/language-extensions"
      ],
      "@drmueller/language-extensions/*": [
        "dist/drmueller/language-extensions/*"
      ],
      "@drmueller/base-services": [
        "dist/drmueller/base-services"
      ],
      "@drmueller/base-services/*": [
        "dist/drmueller/base-services/*"
      ],
      "@drmueller/app-navigation": [
        "dist/drmueller/app-navigation"
      ],
      "@drmueller/app-navigation/*": [
        "dist/drmueller/app-navigation/*"
      ],
      "@drmueller/breadcrumbs": [
        "dist/drmueller/breadcrumbs"
      ],
      "@drmueller/breadcrumbs/*": [
        "dist/drmueller/breadcrumbs/*"
      ],
      "@drmueller/data-access": [
        "dist/drmueller/data-access"
      ],
      "@drmueller/data-access/*": [
        "dist/drmueller/data-access/*"
      ],
      "ng-mat-extensions": [
        "dist/ng-mat-extensions"
      ],
      "ng-mat-extensions/*": [
        "dist/ng-mat-extensions/*"
      ],
      "@drmueller/ng-mat-extensions": [
        "dist/drmueller/ng-mat-extensions"
      ],
      "@drmueller/ng-mat-extensions/*": [
        "dist/drmueller/ng-mat-extensions/*"
      ],
      "error-handling": [
        "dist/error-handling"
      ],
      "error-handling/*": [
        "dist/error-handling/*"
      ],
      "@drmueller/error-handling": [
        "dist/drmueller/error-handling"
      ],
      "@drmueller/error-handling/*": [
        "dist/drmueller/error-handling/*"
      ],
      "@drmueller/security": [
        "dist/drmueller/security"
      ],
      "@drmueller/security/*": [
        "dist/drmueller/security/*"
      ]
    }
  }
}